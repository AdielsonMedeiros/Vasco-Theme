	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#666666",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keywords & Control Flow",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#ff4d4d",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#eebb33"
			}
		},
		{
			"name": "Functions & Methods",
			"scope": ["entity.name.function", "support.function"],
			"settings": {
				"foreground": "#ffffff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variables & Properties",
			"scope": ["variable", "support.variable", "variable.other.object.property"],
			"settings": {
				"foreground": "#dddddd"
			}
		},
		{
			"name": "Classes & Types",
			"scope": ["entity.name.type", "support.class"],
			"settings": {
				"foreground": "#ff6666",
				"fontStyle": ""
			}
		},
		{
			"name": "Constants & Numbers",
			"scope": ["constant.numeric", "constant.language"],
			"settings": {
				"foreground": "#ff9999"
			}
		},
		{
			"name": "HTML Tags / JSX",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#CE1126"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#cccccc",
				"fontStyle": "italic"
			}
		}
	]
}
